//받은 정보를 토대로 서버에 post 군번 : ${Get.arguments['dognum']} , 부대코드 : ${Get.arguments['unitnum']}
void _postRequest(String stockCode) async {
  final response = await http.post(
    Uri.parse(''), // url
    body: <String, String>{
      'serial_num': '${Get.arguments['dognum']}',
      'unit_num': '${Get.arguments['unitnum']}',
    },
  );
  print('response Body : ' + response.body);
  if (response.statusCode == 201 || response.statusCode == 200) {
    print("서버 통신양호 : " + getJson.fromJson(jsonDecode(response.body)).value);
  } else if (response.statusCode == 504) {
    print("서버와의 연결이 불안정 합니다.");
  } else {
    print("종목코드가 올바르지 않습니다.");
    throw Exception('Failed to contect Server.');
  }
}

/////// 서버에서 보낸 json 값 받는 class 코드
class getJSon {
  final double value;
  getPrice({this.value});
  factory getJSon.fromJson(Map<String, dynamic> json) {
    return getJSon(
      value: json['value'],
    );
  }
}